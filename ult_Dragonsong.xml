		<!--ハイジャンプの人数カウント　9秒-->
		<t no="9" sync="^1A:[id8]:[pc] gains the effect of ターゲッティング：ダークハイジャンプ from  for 9.00 Seconds" text="ターゲッティング9_count">
			<expressions>
				<set name="highjump_count_9" value="EVAL(VAR['highjump_count_9']+1,N0)" />
			</expressions>
		</t>
  @{
  
    string[] JobOrder_high = {"DRK", "PLD", "WHM", "SCH", "DRG","NIN","DNC", "RDM"} ; // 並び順
    int priority_9 = 0;
    bool isMyFront_9 = true;
    foreach (string job in JobOrder_high) {
      @:<t no="@priority_9" sync="^1A:[id8]:[@job] gains the effect of ターゲッティング：ダークハイジャンプ from  for 9.00 Seconds" sync-count="1" >
        if (job == Model.Player.Job) { isMyFront_9 = !isMyFront_9; }
        if (isMyFront_9) {
          	@:<expressions>
            	@:<set name="MyPosition_9" count="+1" />
          	@:</expressions>
        }
      @:</t>
      priority_9++;
    }
  }

