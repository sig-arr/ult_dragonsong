		<a time="128" text="時間切れ" />
		<a time="140" text="移行待ち" />

		<t sync="オメガM:ワタシはオメガであり、アルファである" goto="P2" />

		<!--以下P1トリガ-->
		<t no="1" sync="^1B:[id8]:[pc]:0000:[id4]:(?&lt;marker&gt;[0-9a-fA-F]{4}):" >
			<expressions>
				<!--1回目フラグ起動時のみ稼働。稼働と同時にフラグを初期化。-->
				<pre name="get_origin" value="true" />
				<set name="marker_origin" value="0x${marker}" scope="CurrentZone" />
			</expressions>
		</t>

		<t sync="^1A:[id8]:[mex] gains the effect of ファーストターゲット" >
			<v-notice text="1番" duration="15" />
		</t>
		<t sync="^1A:[id8]:[mex] gains the effect of セカンドターゲット" >
			<v-notice text="2番" duration="15" />
		</t>
		<t sync="^1A:[id8]:[mex] gains the effect of サードターゲット" >
			<v-notice text="3番" duration="15" />
		</t>
		<t sync="^1A:[id8]:[mex] gains the effect of フォースターゲット" >
			<v-notice text="4番" duration="15" />
		</t>
	</s>

	<s name="P2">
