  <a enabled="false" time="025.9" text="unknown_7bfe Sign" sync="15:[id8]:オメガ:7BFE:unknown_7bfe:[id8]:[pc]:" />
  -->
		<a time="031.0" text="連携プログラムPT" sync="オメガMの「連携プログラムPT」" />

